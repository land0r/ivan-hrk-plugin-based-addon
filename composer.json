{
	"require": {
		"rdlowrey/auryn": "^1.4"
	},
	"repositories": {
		"repo-name": {
			"type": "vcs",
			"url": "https://github.com/awesomemotive/wpforms-phpcs.git"
		}
	},
	"scripts": {
		"cs": "phpcs --standard=phpcs.xml . -- -s",
		"cbf": "phpcbf ."
	},
	"require-dev": {
		"awesomemotive/wpforms-phpcs": "^1.3"
	},
	"autoload": {
		"psr-4": {
			"Ivan_Api_Based\\": "includes/"
		}
	},
	"config": {
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	}
}
