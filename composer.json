{
	"repositories": {
		"repo-name": {
			"type": "vcs",
			"url": "https://github.com/awesomemotive/wpforms-phpcs.git"
		}
	},
	"scripts": {
		"cs": "phpcs --standard=phpcs.xml . -- -s",
		"cbf": "phpcbf ."
	},
	"require-dev": {
		"awesomemotive/wpforms-phpcs": "^1.3"
	},
	"autoload": {
		"classmap": [
			"includes/"
		]
	},
	"config": {
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true
		},
		"platform": {
			"php": "8.0"
		}
	}
}
